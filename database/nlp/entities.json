{
  "time": {
    "patterns": [
      "\\d{1,2}:\\d{1,2}",
      "\\d{1,2} giờ",
      "\\d{1,2} giờ \\d{1,2}",
      "\\d{1,2} phút",
      "\\d{1,2}h\\d{0,2}",
      "sáng|trưa|chiều|tối|đêm|khuya",
      "hôm nay|ngày mai|hôm qua|tuần tới|tuần trước|tháng này|tháng sau"
    ]
  },
  "date": {
    "patterns": [
      "\\d{1,2}/\\d{1,2}/\\d{2,4}",
      "\\d{1,2}-\\d{1,2}-\\d{2,4}",
      "\\d{1,2} tháng \\d{1,2}( năm \\d{2,4})?",
      "ngày \\d{1,2} tháng \\d{1,2}( năm \\d{2,4})?"
    ]
  },
  "money": {
    "patterns": [
      "\\d+k",
      "\\d+đ",
      "\\d+ nghìn",
      "\\d+ triệu",
      "\\d+ tỷ",
      "\\d+ xu",
      "\\d+VND",
      "\\d+₫",
      "\\d+USD",
      "\\$\\d+"
    ]
  },
  "phone": {
    "patterns": [
      "\\d{10,11}",
      "\\+\\d{1,3}\\d{8,10}"
    ]
  },
  "email": {
    "patterns": [
      "[\\w.-]+@[\\w.-]+\\.[\\w]+"
    ]
  },
  "url": {
    "patterns": [
      "https?://[\\w.-]+\\.[\\w]+(/[\\w./-]*)?",
      "www\\.[\\w.-]+\\.[\\w]+(/[\\w./-]*)?"
    ]
  },
